{
  "name": "q-agency-react-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:style": "stylelint '**/*.scss'",
    "preview": "vite preview",
    "prepare": "husky install"
